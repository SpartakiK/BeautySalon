@model string
<!DOCTYPE html>
<html lang="en">
<head>
</head>
<body id="header" class="header">
    <div class="container">
        <div style="margin-top: 10%; margin-left: 40%; width: 20%">
            <h4>Search user with:</h4>
            <form action="/User/ShowUserInfo" method="get">
                <div class="form-group">

                    <input class="form-control" type="text" name="phonenumber" placeholder="PhoneNumber">
                    <button type="submit" class="btn btn-success">Send</button>

                </div>
            </form>
            <form action="/User/ShowUserInfo" method="get">
                <div class="form-group">

                    <input class="form-control" type="number" name="userid" placeholder="User Id">
                    <button type="submit" class="btn btn-success">Send</button>

                </div>
            </form>
            <a class="btn btn-secondary" href="javascript:history.back()">Go Back</a>
            <p>@Model</p>
        </div>
        </div>
</body>
</html>
